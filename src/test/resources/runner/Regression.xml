<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Presentation for ZeroBanking">
<listeners>

		<listener class-name="listeners.CustomListeners" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />

	</listeners>
  <test thread-count="5" name="Regression">
    <classes>
      <class name="functionaltestcases.AccountSummaryTestCase"/>
      <class name="functionaltestcases.AddPayeeTestCase"/>
      <class name="functionaltestcases.PurchaseFCurrencyTestCase"/>
      <class name="functionaltestcases.ShowTransactionTestCase"/>
      <class name="functionaltestcases.FindTransactionTestCase"/>
       <class name="functionaltestcases.PaySavedPayeeTestCase"/>
      <class name="functionaltestcases.TransferFundTestCase"/>
     
    </classes>
  </test> <!-- Regression -->
</suite> <!-- Suite -->
